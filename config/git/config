[alias]
	co = checkout
	br = branch
	st = status
	lol = log --oneline
	dc = duet-commit
	c = commit
	cane = commit --amend --no-edit
	lolg = log --oneline --graph
	cm = commit -m
	ap = add -p
[pull]
	rebase = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
